DOCUMENT();
loadMacros("PG.pl",
           "PGbasicmacros.pl",
           "PGchoicemacros.pl",
           "PGanswermacros.pl",
           "PGauxiliaryFunctions.pl",
           "PGgraphmacros.pl",
           "Dartmouthmacros.pl");
 

## Do NOT show partial correct answers
$showPartialCorrectAnswers = 1;

## Lots of set up goes here
$a = random(0,5);
$b = random($a+1,$a+6);
$c = random(1,9);
$d = random($c+1,$c+6);


## Ok, we are ready to begin the problem...
##
TEXT(beginproblem());


BEGIN_TEXT
    Find \( \int_{$a}^{$b} \int_{$c}^{$d}( x + \ln y) \,dydx \)
$BR
$BR
Answer = \{ans_rule(60)\}
END_TEXT

$alpha = "($d*(ln($d) - 1) - $c*(ln($c) - 1))";
$ans = "($d - $c)*($b^2 - $a^2)/2 + $alpha*($b - $a)";
   
ANS(num_cmp($ans));
ENDDOCUMENT();

