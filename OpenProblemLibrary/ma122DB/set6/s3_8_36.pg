"PGauxiliaryFunctions.pl"
);

TEXT(beginproblem());
$showPartialCorrectAnswers = 1;

$a = random(2,9,1);
$b = random(2,9,1);
$c = random(2,9,1);
$d = random(2,9,1);
$e = random(2,9,1);

$funct1 = "$a/x +$b/(x+$d)+2*x*$c/(x^2+$e)";

BEGIN_TEXT
Let \[ f(x) = \ln [ x^ {$a} (x + $d)^ {$b} (x^2 + $e)^ {$c} ] \]
$BR$BR
\( f'( x ) = \) \{ans_rule(40) \}
END_TEXT

ANS(fun_cmp($funct1));

